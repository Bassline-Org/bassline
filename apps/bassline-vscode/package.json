{
    "name": "bassline",
    "displayName": "Bassline",
    "description": "Syntax highlighting for Bassline language",
    "version": "0.1.0",
    "engines": {
        "vscode": "^1.60.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [
            {
                "id": "bassline",
                "aliases": [
                    "Bassline",
                    "bassline"
                ],
                "extensions": [
                    ".bl"
                ],
                "configuration": "./language-configuration.json"
            }
        ],
        "grammars": [
            {
                "language": "bassline",
                "scopeName": "source.bassline",
                "path": "./syntaxes/bassline.tmLanguage.json"
            }
        ],
        "snippets": [
            {
                "language": "bassline",
                "path": "./snippets/bassline.json"
            }
        ]
    }
}
