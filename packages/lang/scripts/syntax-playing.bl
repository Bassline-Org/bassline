use functions

for: fn ['each 'in series block] [
    map series fn reduce [cast each word!] block
]

from: fn [start 'to finish] [
    diff: - finish start
    stop: + diff 1
    map iota stop fn [x] compose [+ x (diff)]
]

range: fn [start finish] [
    from start to finish
]

range 10 20

for 'foo in range 10 20 [ print foo ]