use functions
use sockets

client-1: make ws-client!

url: "ws://localhost:8080"
key: "test"

in client-1 []

client-2: make ws-client!

in client-2 [
    value: 123
]

open client-1
open client-2

after (sleep 4000) [
    in client-1 [
        send [print "Hello from client-1"]
    ]
    in client-2 [
        send [print "Hello from client-2"]
    ]
] system