{
  "name": "@bassline/lang",
  "version": "1.0.0",
  "description": "The Bassline language",
  "type": "module",
  "module": "src/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./src/index.js"
    },
    "./prelude": {
      "types": "./dist/prelude/index.d.ts",
      "default": "./src/prelude/index.js"
    },
    "./utils": {
      "types": "./dist/utils.d.ts",
      "default": "./src/utils.js"
    },
    "./runtime": {
      "types": "./dist/runtime.d.ts",
      "default": "./src/runtime.js"
    },
    "./io/file": {
      "types": "./dist/io/file.d.ts",
      "default": "./src/io/file.js"
    },
    "./io/process": {
      "types": "./dist/io/process.d.ts",
      "default": "./src/io/process.js"
    },
    "./io/ws-server": {
      "types": "./dist/io/ws-server.d.ts",
      "default": "./src/io/ws-server.js"
    },
    "./io/ws-client": {
      "types": "./dist/io/ws-client.d.ts",
      "default": "./src/io/ws-client.js"
    },
    "./connections": {
      "default": "./src/connections.js"
    },
    "./server": {
      "default": "./src/server.js"
    },
    "./client": {
      "default": "./src/client.js"
    }
  },
  "bin": {
    "bassline": "./src/bin/cli.js"
  },
  "dependencies": {
    "ws": "^8.0.0",
    "@bassline/parser": "workspace:*"
  },
  "devDependencies": {
    "typescript": "^5.0.0",
    "@types/ws": "^8.0.0"
  },
  "scripts": {
    "test": "vitest run",
    "cli": "node src/bin/cli.js",
    "build": "tsc"
   },
  "license": "MIT"
}