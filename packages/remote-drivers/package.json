    "README.md"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "typecheck": "tsc --noEmit"
  },
  "devDependencies": {
    "tsup": "^8.0.0",
    "typescript": "^5.0.0"
  },
  "dependencies": {
    "@bassline/core": "workspace:*"
  },
  "peerDependencies": {},
  "keywords": ["bassline", "remote", "websocket", "client"],
  "repository": {
    "type": "git",
    "url": "https://github.com/bassline-ai/bassline.git"
