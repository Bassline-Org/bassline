{
  "name": "@bassline/remote-drivers",
  "version": "1.0.0",
  "description": "Bassline remote drivers - WebSocket client bridge for browser and Node.js",
  "type": "module",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
