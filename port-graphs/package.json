{
  "name": "port-graphs",
  "version": "1.0.0",
  "description": "Port-based graph data structure",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./cells": {
      "import": "./dist/patterns/cells/index.js",
      "types": "./dist/patterns/cells/index.d.ts"
    },
    "./functions": {
      "import": "./dist/patterns/functions/index.js",
      "types": "./dist/patterns/functions/index.d.ts"
    },
    "./meta": {
      "import": "./dist/patterns/meta/index.js",
      "types": "./dist/patterns/meta/index.d.ts"
    },
    "./core": {
      "import": "./dist/core/index.js",
      "types": "./dist/core/index.d.ts"
    },
    "./effects": {
      "import": "./dist/effects/index.js",
      "types": "./dist/effects/index.d.ts"
    },
    "./ui": {
      "import": "./dist/patterns/ui/index.js",
      "types": "./dist/patterns/ui/index.d.ts"
    }
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "test": "vitest --run"
  },
  "license": "MIT",
  "devDependencies": {
    "@types/js-yaml": "^4.0.9",
    "@types/lodash": "^4.17.20",
    "@types/node": "^22.0.0",
    "typescript": "^5.3.0",
    "vitest": "^1.2.0"
  },
  "dependencies": {
    "js-yaml": "^4.1.0",
    "lodash": "^4.17.21"
  }
}
