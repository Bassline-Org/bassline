{
  "name": "bassline",
  "version": "0.1.0",
  "private": true,
  "description": "Getchyo groove on",
  "repository": {
    "type": "git",
    "url": "https://github.com/MercuricChloride/bassline.git"
  },
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0",
    "pnpm": ">=8.0.0"
  },
  "packageManager": "pnpm@8.15.0",
  "scripts": {
    "start": "pnpm --filter @bassline/web start",
    "dev": "pnpm --parallel --stream dev",
    "dev:web": "pnpm --filter @bassline/web dev",
    "build": "pnpm -r build",
    "test": "pnpm -r test",
    "typecheck": "pnpm -r typecheck",
    "clean": "pnpm -r clean && rm -rf node_modules",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^4.1.1",
    "@storybook/addon-a11y": "^9.1.7",
    "@storybook/addon-docs": "^9.1.7",
    "@storybook/addon-onboarding": "^9.1.7",
    "@storybook/addon-vitest": "^9.1.7",
    "@storybook/react": "^9.1.7",
    "@storybook/react-vite": "^9.1.7",
    "@types/node": "^20.19.9",
    "@types/react": "^19.1.13",
    "@vitejs/plugin-react": "^4.7.0",
    "storybook": "^9.1.7",
    "typescript": "^5.3.3",
    "vitest": "^3.2.4"
  }
}