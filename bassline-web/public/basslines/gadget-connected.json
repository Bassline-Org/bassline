{
  "name": "Connected Gadget Example",
  "description": "A network with gadgets connected via boundary contacts",
  "version": "1.0",
  "author": "Bassline",
  "createdAt": "2024-01-01T00:00:00Z",
  "rootGroup": {
    "name": "Root",
    "contacts": [
      {
        "position": { "x": 100, "y": 200 },
        "isBoundary": false,
        "blendMode": "accept-last",
        "content": {
          "@bassline-type@": "Set",
          "data": ["apple", "banana", "cherry"]
        },
        "name": "Input Data"
      },
      {
        "position": { "x": 700, "y": 200 },
        "isBoundary": false,
        "blendMode": "accept-last",
        "name": "Processed Output"
      }
    ],
    "wires": [
      {
        "fromIndex": 0,
        "toIndex": -1,
        "type": "bidirectional",
        "toSubgroupIndex": 0,
        "toBoundaryName": "in"
      },
      {
        "fromIndex": -1,
        "toIndex": 1,
        "type": "bidirectional",
        "fromSubgroupIndex": 0,
        "fromBoundaryName": "out"
      }
    ],
    "subgroupTemplates": [
      {
        "name": "Processing Gadget",
        "contacts": [
          {
            "position": { "x": 50, "y": 100 },
            "isBoundary": true,
            "boundaryDirection": "input",
            "name": "in",
            "blendMode": "accept-last"
          },
          {
            "position": { "x": 200, "y": 100 },
            "isBoundary": false,
            "blendMode": "merge",
            "content": {
              "@bassline-type@": "Map",
              "data": [["transform", "uppercase"], ["filter", "fruits"]]
            },
            "name": "Processor Logic"
          },
          {
            "position": { "x": 350, "y": 100 },
            "isBoundary": true,
            "boundaryDirection": "output",
            "name": "out",
            "blendMode": "accept-last"
          }
        ],
        "wires": [
          {
            "fromIndex": 0,
            "toIndex": 1,
            "type": "bidirectional"
          },
          {
            "fromIndex": 1,
            "toIndex": 2,
            "type": "bidirectional"
          }
        ],
        "subgroupTemplates": [],
        "boundaryIndices": [0, 2]
      }
    ],
    "boundaryIndices": []
  }
}